{
  "name": "herowars-helper",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "react-router build && node netlify/prepare.js",
    "dev": "npx supabase status > /dev/null 2>&1 && npx supabase status || (npx supabase start && npx supabase status) && cross-env NODE_ENV=development node ./dev-server.js",
    "start": "netlify serve",
    "tsc": "react-router typegen && tsc",
    "supabase:types": "supabase gen types typescript --local > app/types/supabase.ts",
    "db:diff": "supabase db diff -f",
    "db:reset": "supabase db reset",
    "db:status": "supabase status",
    "db:start": "supabase db start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "prettier . --write --ignore-path .gitignore",
    "checks": "react-router typegen && tsc && vitest run"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@netlify/functions": "^4.2.5",
    "@react-router/node": "^7.9.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/vite": "^4.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.23.16",
    "isbot": "^5.1.30",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1",
    "slugify": "^1.6.6",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@mjackson/node-fetch-server": "0.7.0",
    "@react-router/dev": "^7.9.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.0.0",
    "express": "^5.1.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "netlify-cli": "^23.6.0",
    "postcss": "^8.5.6",
    "react-router-devtools": "^5.1.3",
    "supabase": "^2.40.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "vite": "^7.1.11",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "overrides": {
    "vite-plugin-devtools-json": {
      "vite": "$vite"
    }
  }
}